{
   "scripts": {
      "open": "wslview http://localhost:3000",
      "start:dev": "rm -rf .cache && NODE_ENV=dev NODE_PATH=./ ts-node-dev -r tsconfig-paths/register --transpile-only --cache-directory src/.cache src/*.ts --watch",
      "build": "rm -rf dist && NODE_PATH=./src/dist tsc",
      "start:prod": "npm run build && node dist/app.js"
   },
   "dependencies": {
      "dotenv": "^10.0.0",
      "express": "^4.17.1",
      "mongoose": "^5.13.5"
   },
   "devDependencies": {
      "@types/express": "^4.17.13",
      "@types/node": "^16.4.12",
      "@typescript-eslint/eslint-plugin": "^4.29.0",
      "@typescript-eslint/parser": "^4.29.0",
      "eslint": "^7.32.0",
      "eslint-config-standard": "^16.0.3",
      "eslint-plugin-import": "^2.23.4",
      "eslint-plugin-node": "^11.1.0",
      "eslint-plugin-promise": "^5.1.0",
      "ts-node": "^10.1.0",
      "ts-node-dev": "^1.1.8",
      "tsconfig-paths": "^3.10.1",
      "typescript": "^4.3.5"
   }
}
